{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/**/*",
    "src/services/**/*",
    "src/actions/**/*",
    "src/config/config.json"
  ],
  "compilerOptions": {
    "composite": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": [
      "electron-vite/node"
    ],
    "outDir": "out" 
  }
}